    "@qiskit-community/qiskit-vue": "^0.23.0",
    "@vue/composition-api": "^1.0.0-beta.1",
    "carbon-components": "^10.34.0",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "nuxt": "^2.14.12",
    "nuxt-lazy-load": "^1.2.6",
    "ts-node": "^9.1.1",
    "vue-class-component": "^7.2.6",
    "vue-demi": "^0.9.1",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
