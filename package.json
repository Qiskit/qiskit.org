{
  "name": "qiskit.org",
  "version": "1.0.0",
  "private": true,
  "description": "Qiskit is an open source software development kit (SDK) for working with OpenQASM and the IBM Q quantum processors. Create quantum computing programs, compile, and execute them online in a real quantum processors.",
  "repository": "Qiskit/qiskit.org",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "NODE_ENV=development nuxt",
    "dev-analytics": "ENABLE_ANALYTICS=true NODE_ENV=development nuxt",
    "dev-debug": "node --inspect node_modules/.bin/nuxt",
    "test": "jest",
    "build": "nuxt build",
    "start": "NODE_ENV=production nuxt start",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "eslint --max-warnings 0 --ext .ts,.js,.vue . && npm run lint:style",
    "generate": "NODE_ENV=production nuxt generate --fail-on-error",
    "fix-lint": "eslint --fix --ext .ts,.js,.vue . && npm run lint:style -- --fix"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@carbon/colors": "~10.30.0",
    "@carbon/icons-vue": "~10.41.0",
    "@carbon/themes": "~10.45.0",
    "@carbon/type": "~10.33.0",
    "@carbon/vue": "~2.37.1",
    "@nuxt/content": "^1.14.0",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@qiskit-community/qiskit-vue": "^3.4.0",
    "@vue/composition-api": "^1.4.2",
    "airtable": "~0.11.1",
    "cross-env": "^7.0.3",
    "frontmatter-markdown-loader": "^3.6.3",
    "lodash": "^4.17.21",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-link-attributes": "^3.0.0",
    "nuxt": "^2.15.8",
    "nuxt-lazy-load": "^1.2.7",
    "uslug": "^1.0.4",
    "vue-class-component": "^7.2.6",
    "vue-demi": "^0.11.4",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.172",
    "@types/markdown-it": "^12.2.0",
    "@types/markdown-it-anchor": "^7.0.0",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/uslug": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.16.0",
    "jest": "^27.3.1",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "sass-loader": "^10.2.0",
    "sass-mq": "^5.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "ts-jest": "^27.0.7",
