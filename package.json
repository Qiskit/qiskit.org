{
  "name": "qiskit.org",
  "version": "1.0.0",
  "private": true,
  "description": "Qiskit is an open-source SDK for working with quantum computers at the level of pulses, circuits, and application modules.",
  "repository": "Qiskit/qiskit.org",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "NODE_ENV=development nuxt",
    "dev-analytics": "ENABLE_ANALYTICS=true NODE_ENV=development nuxt",
    "dev-debug": "node --inspect node_modules/.bin/nuxt",
    "test": "jest",
    "test:changed": "jest -o",
    "build": "nuxt build",
    "start": "NODE_ENV=production nuxt start",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "eslint --max-warnings 0 --ext .ts,.js,.vue . && npm run lint:style",
    "generate": "NODE_ENV=production nuxt generate --fail-on-error",
    "fix-lint": "eslint --fix --ext .ts,.js,.vue . && npm run lint:style -- --fix"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@carbon/colors": "~10.37.1",
    "@carbon/icons-vue": "~10.49.1",
    "@carbon/themes": "~11.13.0",
    "@carbon/type": "~10.33.0",
    "@carbon/vue": "2.44.0",
    "@nuxt/content": "^1.14.0",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@open-wc/webpack-import-meta-loader": "^0.4.7",
    "@qiskit-community/qiskit-vue": "^3.4.0",
    "@qiskit/web-components": "^0.11.0",
    "@vue/composition-api": "^1.4.9",
    "airtable": "~0.11.4",
    "cross-env": "^7.0.3",
    "frontmatter-markdown-loader": "^3.7.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-link-attributes": "^4.0.1",
    "nuxt": "^2.15.8",
    "nuxt-lazy-load": "^1.2.9",
    "uslug": "^1.0.4",
    "vue-class-component": "^7.2.6",
    "vue-demi": "^0.13.11",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.184",
    "@types/markdown-it": "^12.2.0",
    "@types/markdown-it-anchor": "^7.0.0",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/uslug": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.6.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.20",
    "sass": "^1.54.8",
    "sass-loader": "^10.2.0",
    "sass-mq": "^6.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
